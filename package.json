{
  "name": "otus-javascript-weather-forecast",
  "description": "Weather Forecast App",
  "keywords": [
    "typescript"
  ],
  "homepage": "https://github.com/alexey-sidorov-dev/otus-javascript-weather-forecast#readme",
  "bugs": {
    "url": "https://github.com/alexey-sidorov-dev/otus-javascript-weather-forecast/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexey-sidorov-dev/otus-javascript-weather-forecast.git"
  },
  "license": "ISC",
  "author": "Alexey Sidorov",
  "main": "./src/index.ts",
  "scripts": {
    "build": "webpack build --define-process-env-node-env production",
    "build:analyze": "webpack build --define-process-env-node-env production && webpack --analyze",
    "dev": "webpack serve --define-process-env-node-env development --open",
    "lint": "eslint . --ext .js,.ts && tsc --project tsconfig.json --noEmit",
    "lint:all": "eslint . --ext .js,.ts && tsc --project tsconfig.json --noEmit && stylelint **/*.{css,scss} && prettier . --check",
    "lint:all:fix": "eslint . --ext .js,.ts --fix && tsc --project tsconfig.json --noEmit && stylelint **/*.{css,scss} --fix && prettier . --write",
    "lint:fix": "eslint . --ext .js,.ts --fix",
    "ncu": "npm-check-updates --interactive --format group --doctor",
    "prepare": "husky install",
    "prettier": "prettier . --check",
    "prettier:fix": "prettier . --write",
    "stylelint": "stylelint **/*.{css,scss}",
    "stylelint:fix": "stylelint **/*.{css,scss} --fix",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "leaflet": "^1.9.3",
    "modern-css-reset": "^1.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.3",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.21.0",
    "@types/config": "^3.3.0",
    "@types/dotenv-webpack": "^7.0.3",
    "@types/eslint": "^8.21.2",
    "@types/jest": "^29.5.0",
    "@types/jsdom": "^21.1.0",
    "@types/leaflet": "^1.9.3",
    "@types/node": "^18.15.3",
    "@types/prettier": "^2.7.2",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "babel-loader": "^9.1.2",
    "copy-webpack-plugin": "^11.0.0",
    "core-js": "^3.29.1",
    "css-loader": "^6.7.3",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.36.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-fetch-mock": "^3.0.3",
    "lint-staged": "^13.2.0",
    "mini-css-extract-plugin": "^2.7.5",
    "npm-check-updates": "^16.7.12",
    "prettier": "^2.8.4",
    "prettier-plugin-packagejson": "^2.4.3",
    "prettier-plugin-sh": "^0.12.8",
    "sass": "^1.59.3",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.2",
    "stylelint": "^15.3.0",
    "stylelint-config-standard-scss": "^7.0.1",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "webpack": "^5.76.2",
    "webpack-bundle-analyzer": "^4.8.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.12.0"
  }
}
